###Alias Section
alias fullscreenshot='xfce4-screenshooter -f'
alias clipscreenshot='xfce4-screenshooter -r'
alias list="ls -a"
alias up="cd .."
alias home="cd ~"
alias disk="cd /"
alias back='cd "$OLDPWD"'
alias work="nano ~/Desktop/turboterm/.TurboTerm"
alias makeprod='cp ~/Desktop/turboterm/.TurboTerm ~/.TurboTerm'
alias xx="exit"

###Welcome Screen
clear
echo -e "'You must begin where you are and with what you are.' - Russell Conwell"
echo -e ""
echo -ne "Today is "; date
echo -e ""; cal ;
echo -ne "Uptime: "; uptime | awk /'up/ '
